define("artdeco-pagination/utils/artdeco-pagination-utils",["exports"],function(e){"use strict"
Object.defineProperty(e,"__esModule",{value:!0})
e.createTruncatedPagination=e.createPaginationArray=void 0
e.createPaginationArray=(e=>{const t=[]
for(let n=0;n<e;n+=1)t.push(n)
return t})
e.createTruncatedPagination=((e,t,n)=>{const i=t-2,o=[]
if(e-n<=i)for(let a=0;a<t;a++)o.push({isEllipsis:1===a,index:a<1?a:e-t+a})
else if(n<i)for(let a=0;a<t;a++)o.push({isEllipsis:a===t-2,index:a===t-1?e-1:a})
else for(let a=0;a<t;a++){let i
i=a<1?a:a===t-1?e-1:a+n-Math.floor(t/2)
o.push({isEllipsis:1===a||a===t-2,index:i})}return o})})
